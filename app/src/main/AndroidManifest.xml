<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="examplewtest.midopc.smartclinic"
    >
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activites.Newpatiant"
            android:label="@string/title_activity_newpatiant"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activites.Patiants" />
        <activity
            android:name=".Activites.Status"
            android:label="@string/title_activity_patient__profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activites.StatusForm"
            android:label="@string/title_activity_status_form"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
